{
  "relation": {
    "hover": {
      "prefix": "h",
      "list": {
        "flip": [
          "button",
          "picture",
          "icon"
        ],
        "fade-in": {
          "models": [
            "button",
            "picture",
            "text"
          ],
          "modifiers": {
            "opacity-from": {
              "type": "select",
              "label": "From Opacity",
              "options": [
                {
                  "value": "00",
                  "label": "0%",
                  "class": "my-h-fade-from-00"
                },
                {
                  "value": "01",
                  "label": "10%",
                  "class": "my-h-fade-from-01"
                },
                {
                  "value": "02",
                  "label": "20%",
                  "class": "my-h-fade-from-02"
                },
                {
                  "value": "03",
                  "label": "30% (Default)",
                  "class": ""
                },
                {
                  "value": "04",
                  "label": "40%",
                  "class": "my-h-fade-from-04"
                },
                {
                  "value": "05",
                  "label": "50%",
                  "class": "my-h-fade-from-05"
                }
              ],
              "default": "10"
            },
            "opacity-to": {
              "type": "select",
              "label": "To Opacity",
              "options": [
                {
                  "value": "06",
                  "label": "60%",
                  "class": "my-h-fade-to-06"
                },
                {
                  "value": "07",
                  "label": "70%",
                  "class": "my-h-fade-to-07"
                },
                {
                  "value": "08",
                  "label": "80%",
                  "class": "my-h-fade-to-08"
                },
                {
                  "value": "09",
                  "label": "90%",
                  "class": "my-h-fade-to-09"
                },
                {
                  "value": "10",
                  "label": "100% (Default)",
                  "class": "my-h-fade-to-10"
                }
              ],
              "default": "10"
            },
            "speed": {
              "type": "select",
              "label": "Animation Speed",
              "options": [
                {
                  "value": "fast",
                  "label": "Fast (0.15s)",
                  "class": "my-h-fade-duration-fast"
                },
                {
                  "value": "normal",
                  "label": "Normal (0.3s)",
                  "class": ""
                },
                {
                  "value": "slow",
                  "label": "Slow (0.6s)",
                  "class": "my-h-fade-duration-slow"
                },
                {
                  "value": "slower",
                  "label": "Slower (1s)",
                  "class": "my-h-fade-duration-slower"
                }
              ],
              "default": "normal"
            }
          }
        },
        "fade-out": [
          "button",
          "picture",
          "text"
        ],
        "fade-partial": [
          "button",
          "picture",
          "text"
        ],
        "fade-up": [
          "button",
          "picture",
          "text"
        ],
        "fade-down": [
          "button",
          "picture",
          "text"
        ],
        "fade-left": [
          "button",
          "picture",
          "text"
        ],
        "fade-right": {
          "models": [
            "button",
            "picture",
            "text"
          ],
          "class": {
            "effectClass": "my-h-fade-right my-h-fade-warning"
          }
        },
        "fade-grow": [
          "button",
          "picture",
          "text"
        ],
        "fade-shrink": [
          "button",
          "picture",
          "text"
        ],
        "fade-pulse": [
          "button",
          "picture",
          "text"
        ],
        "fade-glow": [
          "button",
          "picture",
          "text"
        ],
        "fade-blur": [
          "button",
          "picture",
          "text"
        ],
        "fade-saturate": [
          "button",
          "picture",
          "text"
        ],
        "fade-twist": [
          "button",
          "picture",
          "text"
        ],
        "fade-slide": [
          "button",
          "picture",
          "text"
        ],
        "fade-bounce": [
          "button",
          "picture",
          "text"
        ],
        "rotate": [
          "button",
          "picture",
          "text",
          "icon"
        ],
        "zoom": [
          "picture"
        ],
        "special": {
          "models": [
            "button",
            "picture",
            "special-element"
          ],
          "class": {
            "effectClass": "my-custom-class another-class",
            "nextClass": "my-next-class"
          },
          "html": {
            "special-element": "<div class='my-custom-class {effectClass}'>Special!</div><div class='{nextClass}'></div>"
          }
        },
        "example-flip": [
          "example-box"
        ],
        "example-fade": [
          "example-box"
        ]
      }
    },
    "click": {
      "prefix": "c",
      "list": {
        "flip": [
          "button",
          "picture",
          "text",
          "icon"
        ],
        "rotate": [
          "button"
        ],
        "zoom": [
          "picture"
        ]
      }
    },
    "drag": {
      "prefix": "d",
      "list": {
        "zoom": [
          "picture"
        ]
      }
    }
  },
  "model": {
    "button": "<button class=\"my-btn {effectClass}\">Button</button>",
    "picture": "<img src=\"https://images.unsplash.com/photo-1506744038136-46273834b3fb?auto=format&fit=crop&w=400&q=80\" alt=\"Demo\" class=\"my-img {effectClass}\">",
    "text": "<span class=\"my-text {effectClass}\">Sample Text</span>",
    "icon": "<i class=\"my-icon {effectClass}\" aria-label=\"icon\">â˜…</i>",
    "example-box": "<div class=\"manyang-example-box {effectClass}\">Example Box</div>"
  }
}