{
  "relation": {
    "hover": {
      "prefix": "h",
      "list": {
        "flip": ["button", "picture", "icon"],
        "fade-in": {
          "models": ["button", "picture", "text"],
          "modifiers": {
            "opacity": {
              "type": "select",
              "label": "Target Opacity",
              "options": [
                {"value": "01", "label": "10%", "class": "manyang-h-fade-opacity-01"},
                {"value": "03", "label": "30%", "class": "manyang-h-fade-opacity-03"},
                {"value": "05", "label": "50%", "class": "manyang-h-fade-opacity-05"},
                {"value": "07", "label": "70%", "class": "manyang-h-fade-opacity-07"},
                {"value": "09", "label": "90%", "class": "manyang-h-fade-opacity-09"},
                {"value": "10", "label": "100% (Default)", "class": ""}
              ],
              "default": "10"
            },
            "speed": {
              "type": "select",
              "label": "Animation Speed", 
              "options": [
                {"value": "fast", "label": "Fast (0.15s)", "class": "manyang-h-fade-duration-fast"},
                {"value": "normal", "label": "Normal (0.3s)", "class": ""},
                {"value": "slow", "label": "Slow (0.6s)", "class": "manyang-h-fade-duration-slow"},
                {"value": "slower", "label": "Slower (1s)", "class": "manyang-h-fade-duration-slower"}
              ],
              "default": "normal"
            }
          }
        },
        "fade-out": ["button", "picture", "text"],
        "fade-partial": ["button", "picture", "text"],
        "fade-up": ["button", "picture", "text"],
        "fade-down": ["button", "picture", "text"],
        "fade-left": ["button", "picture", "text"],
        "fade-right": ["button", "picture", "text"],
        "fade-grow": ["button", "picture", "text"],
        "fade-shrink": ["button", "picture", "text"],
        "fade-pulse": ["button", "picture", "text"],
        "fade-glow": ["button", "picture", "text"],
        "fade-blur": ["button", "picture", "text"],
        "fade-saturate": ["button", "picture", "text"],
        "fade-twist": ["button", "picture", "text"],
        "fade-slide": ["button", "picture", "text"],
        "fade-bounce": ["button", "picture", "text"],
        "rotate": ["button", "picture", "text", "icon"],
        "zoom": ["picture"],
        "special": {
          "models": ["button", "picture", "special-element"],
          "class": {
            "effectClass": "my-custom-class another-class",
            "nextClass": "my-next-class"
          },
          "html": {
            "special-element": "<div class='my-custom-class {effectClass}'>Special!</div><div class='{nextClass}'></div>"
          }
        }
      }
    },
    "click": {
      "prefix": "c",
      "list": {
        "flip": ["button", "picture", "text", "icon"],
        "rotate": ["button"],
        "zoom": ["picture"]
      }
    },
    "drag": {
      "prefix": "d",
      "list": {
        "zoom": ["picture"]
      }
    }
  },
  "model": {
    "button": "<button class=\"manyang-btn {effectClass}\">Button</button>",
    "picture": "<img src=\"https://images.unsplash.com/photo-1506744038136-46273834b3fb?auto=format&fit=crop&w=400&q=80\" alt=\"Demo\" class=\"manyang-img {effectClass}\">",
    "text": "<span class=\"manyang-text {effectClass}\">Sample Text</span>",
    "icon": "<i class=\"manyang-icon {effectClass}\" aria-label=\"icon\">â˜…</i>"
  }
}
